<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/close"
            android:layout_width="@dimen/_18sdp"
            android:layout_height="@dimen/_280sdp"
            android:layout_alignParentLeft="true"
            android:background="@color/colorPrimary"
            android:paddingLeft="@dimen/_4sdp"
            android:text="@string/greater_than"
            android:textColor="#000000"
            android:textSize="15sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/sin"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_below="@id/inv"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="sin"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <Button
            android:id="@+id/cos"
            android:layout_width="@dimen/_56sdp"

            android:layout_height="@dimen/_56sdp"
            android:layout_below="@+id/sin"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="cos"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <Button
            android:id="@+id/tan"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_below="@+id/cos"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="tan"

            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />


        <Button
            android:id="@+id/cosec"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_alignTop="@+id/sin"
            android:layout_toEndOf="@+id/sin"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="cosec"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <Button
            android:id="@+id/sec"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_below="@+id/sin"
            android:layout_toEndOf="@+id/sin"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="sec"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <Button
            android:id="@+id/inv"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="inv"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <Button
            android:id="@+id/cot"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_below="@+id/cos"
            android:layout_toEndOf="@+id/sin"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="cot"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />


        <Button
            android:id="@+id/rad"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/sin"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="RAD"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <Button
            android:id="@+id/pi"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_below="@+id/tan"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:text="@string/pi"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="@dimen/_20sdp" />

        <ImageButton
            android:id="@+id/backsp_"
            android:layout_width="@dimen/_56sdp"
            android:layout_height="@dimen/_56sdp"
            android:layout_below="@+id/tan"
            android:layout_toEndOf="@+id/sin"
            android:layout_toRightOf="@+id/close"
            android:background="#b8faff"
            android:scaleType="centerCrop"
            android:src="@drawable/back" />


    </RelativeLayout>


</FrameLayout>